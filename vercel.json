{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build/client"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/orderly/:path*",
      "destination": "https://testnet-api-evm.orderly.org/v1/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
